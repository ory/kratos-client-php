{
  "name": "ory/kratos-client-php",
  "description": "This is the API specification for Ory Identities with features such as registration, login, recovery, account verification, profile settings, password reset, identity management, session management, email and sms delivery, and more.",
  "keywords": [
    "openapitools",
    "openapi-generator",
    "openapi",
    "php",
    "sdk",
    "rest",
    "api"
  ],
  "homepage": "https://github.com/ory/kratos-client-php",
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "Ory GmbH",
      "homepage": "https://www.ory.sh"
    }
  ],
  "require": {
    "php": "^7.3 || ^8.0",
    "ext-curl": "*",
    "ext-json": "*",
    "ext-mbstring": "*",
    "guzzlehttp/guzzle": "^7.3",
    "guzzlehttp/psr7": "^1.7 || ^2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.0 || ^9.0",
    "friendsofphp/php-cs-fixer": "^2.12"
  },
  "autoload": {
    "psr-4": {
      "Ory\\Kratos\\Client\\": "lib/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Ory\\Kratos\\Client\\Test\\": "test/"
    }
  }
}
